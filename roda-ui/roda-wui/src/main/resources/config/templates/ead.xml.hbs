<?xml version="1.0" encoding="utf-8"?>
<ead xmlns="urn:isbn:1-931666-22-9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:isbn:1-931666-22-9 ../schemas/ead2002.xsd">

  <eadheader langencoding="iso639-2b" countryencoding="iso3166-1" dateencoding="iso8601" repositoryencoding="iso15511" scriptencoding="iso15924" relatedencoding="DC">
    <eadid encodinganalog="identifier">{{id}}</eadid>
    <filedesc>
      <titlestmt>
        <titleproper encodinganalog="title"></titleproper>
      </titlestmt>
    </filedesc>
        <profiledesc>
            <creation>Generated by RODA - Repository of Authentica Digital Objects</creation>
            {{~#if rules~}}
            <descrules encodinganalog="3.7.2">{{rules}}</descrules>
            {{~/if~}}
        </profiledesc>
    </eadheader>

    <archdesc {{#if level}}level='{{level}}'{{/if}} {{#if otherlevel}}otherlevel="{{otherlevel}}"{{/if}}>
        <did>

            {{! UnitTitle }}
            {{~#if title~}}
                <unittitle encodinganalog="3.1.2" {{#if titleType}}type="{{titleType}}"{{/if}}>{{title}}</unittitle>
            {{~/if~}}

            {{! Unit Id }}
            {{~#ifCond "||" countryCode repositoryCode id~}}
                <unitid encodinganalog="3.1.1" {{#if countryCode}}countrycode="{{countryCode}}"{{/if}} {{#if repositoryCode}}repositorycode="{{repositoryCode}}"{{/if}}>{{id}}</unitid>
            {{~/ifCond~}}


            {{! Dates }}
            {{~#if dateInitial~}}
                {{~#if dateFinal~}}
                    <unitdate encodinganalog="3.1.3" label="UnitDates" normal='{{dateInitial}}/{{dateFinal}}'>{{#if descDate}}{{descDate}}{{/if}}</unitdate>
                {{~^~}}
                    <unitdate encodinganalog="3.1.3" label="UnitDateInitial" normal="{{dateInitial}}">{{#if descDate}}{{descDate}}{{/if}}</unitdate>
                {{~/if~}}
            {{~^~}}
                {{~#if dateFinal~}}
                    <unitdate encodinganalog="3.1.3" label="UnitDateFinal" normal="{{dateFinal}}">{{#if descDate}}{{descDate}}{{/if}}</unitdate>
                {{~^~}}
                    {{~#if descDate~}}
                        <unitdate encodinganalog="3.1.3" label="UnitDates">{{descDate}}</unitdate>
                    {{~/if~}}
                {{~/if~}}
            {{~/if~}}




            {{! Physical Description }}
            {{~#ifCond "||" extent dimensions~}}
                <physdesc encodinganalog="3.1.5">
                    {{~#if extent~}}
                        <extent>{{extent}}</extent>
                    {{~/if~}}

                    {{~#if dimensions~}}
                        <dimensions>{{dimensions}}</dimensions>
                    {{~/if~}}
                </physdesc>
            {{~/ifCond~}}


            {{~#if repository~}}
                <repository>
                    <corpname>{{repository}}</corpname>
                </repository>
            {{~/if~}}


            {{! langmaterial }}
            {{~#ifCond "||" language languageScriptNotes~}}
            <langmaterial>
                {{~#if language~}}
                  <language>{{language}}</language>
                {{~/if~}}

                {{languageScriptNotes}}
            </langmaterial>
            {{~/ifCond~}}


            {{~#if sources~}}
                <note type="sourcesDescription">
                    <p>{{sources}}</p>
                </note>
            {{~/if~}}


            {{~#if notes~}}
                <note type="generalNote">
                    <p>{{notes}}</p>
                </note>
            {{~/if~}}


            {{~#if creator~}}
                <origination encodinganalog="3.2.1" label="creator">
                    <name>{{creator}}</name>
                </origination>
            {{~/if~}}


            {{~#if producer~}}
                <origination label='producer'>
                    <name>{{producer}}</name>
                </origination>
            {{~/if~}}


            {{~#if materialspec~}}
                <materialspec>{{materialspec}}</materialspec>
            {{~/if~}}



        </did>
        {{! closes did }}




        {{! Description Controll Area Fields }}
        {{~#if publicationStatus~}}
            <odd type="publicationStatus">
                <p>{{publicationStatus}}</p>
            </odd>
        {{~/if~}}



        {{~#if levelOfDetail~}}
            <odd type="levelOfDetail">
                <p>{{levelOfDetail}}</p>
            </odd>
        {{~/if~}}


        {{~#if statusDescription~}}
            <odd type="statusDescription">
                <p>{{statusDescription}}</p>
            </odd>
        {{~/if~}}



        {{~#if scopeContent~}}
            <scopecontent encodinganalog="3.3.1">
                <p>{{scopeContent}}</p>
            </scopecontent>
        {{~/if~}}


        {{~#if bioghist~}}
            <bioghist>
                <p>{{bioghist}}</p>
            </bioghist>
        {{~/if~}}


        {{~#if arrangement~}}
            <arrangement encodinganalog="3.3.4">
                <p>{{arrangement}}</p>
            </arrangement>
        {{~/if~}}



        {{~#if phystech~}}
            <phystech encodinganalog="3.4.3">
                <p>{{phystech}}</p>
            </phystech>
        {{~/if~}}


        {{~#if appraisal~}}
            <appraisal encodinganalog="3.3.2">
                <p>{{appraisal}}</p>
            </appraisal>
        {{~/if~}}


        {{~#if acqinfo~}}
            <acqinfo encodinganalog="3.2.4">
                <p>{{acqinfo}}</p>
            </acqinfo>
        {{~/if~}}


        {{~#if accruals~}}
            <accruals encodinganalog="3.3.3">
                <p>{{accruals}}</p>
            </accruals>
        {{~/if~}}


        {{~#if custodhist~}}
            <custodhist encodinganalog="3.2.3">
                <p>{{custodhist}}</p>
            </custodhist>
        {{~/if~}}




        {{~#ifCond "||"  archivistNotes processInfoDate~}}
            <processinfo>
                {{~#if processInfoDate~}}
                  <p><date>{{processInfoDate}}</date></p>
                {{~/if~}}
                <p>{{archivistNotes}}</p>
            </processinfo>
        {{~/ifCond~}}


        {{~#if originalsloc~}}
            <originalsloc encodinganalog="3.5.1">
                <p>{{originalsloc}}</p>
            </originalsloc>
        {{~/if~}}


        {{~#if altformavail~}}
            <altformavail encodinganalog="3.5.2">
                <p>{{altformavail}}</p>
            </altformavail>
        {{~/if~}}


        {{~#if relatedmaterial~}}
            <relatedmaterial encodinganalog="3.5.3">
                <p>{{relatedmaterial}}</p>
            </relatedmaterial>
        {{~/if~}}


        {{~#if accessrestrict~}}
            <accessrestrict encodinganalog="3.4.1">
                <p>{{accessrestrict}}</p>
            </accessrestrict>
        {{~/if~}}


        {{~#if userestrict~}}
            <userestrict encodinganalog="3.4.2">
                <p>{{userestrict}}</p>
            </userestrict>
        {{~/if~}}


        {{~#if otherfindaid~}}
            <otherfindaid encodinganalog="3.4.5">
                <p>{{otherfindaid}}</p>
            </otherfindaid>
        {{~/if~}}


        {{~#if bibliography~}}
            <bibliography encodinganalog="3.5.4">
                <p>{{bibliography}}</p>
            </bibliography>
        {{~/if~}}


        <dsc type="combined">
        </dsc>

    </archdesc>
</ead>
